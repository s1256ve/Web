function chkValue(){
	if (eval(document.IP0706000.TXN_AMT.value) < 10000 || eval(document.IP0706000.TXN_AMT.value) > 5000000){
		alert("單筆轉存金額不得小於1萬元或大於500萬元");
		return;
	}
}

function chkSubmit(){
                     
        //alert(document.IP0706000.SEND_RATE.value);                     
        
           
	var acct = document.IP0706000.SEND_ACCT_NO.value;
	if (acct==""){
		alert("請選擇轉出帳號!");
		return;
	}
	if (document.IP0706000.TXN_AMT.value ==""){
		alert("請輸入轉存金額");
		return;
	}
	if (isNaN(document.IP0706000.TXN_AMT.value)){
            		alert("轉帳金額格式錯誤!");
            		return;
  	}
  	if(document.IP0706000.TXN_AMT.value.indexOf(".",0)!=-1){
	    	alert("轉帳金額須為正整數!");
	    	return;   
	}
	if(document.IP0706000.TXN_AMT.value.indexOf("-",0)!=-1){
	    	alert("轉帳金額須為正整數!");
	    	return;   
	}
	if(document.IP0706000.TXN_AMT.value.indexOf("+",0)!=-1){
	    	alert("台幣轉帳不可帶正號!");
	    	return;   
	}
	if (eval(document.IP0706000.TXN_AMT.value) < 10000 || eval(document.IP0706000.TXN_AMT.value) > 5000000){
		alert("單筆轉存金額不得小於1萬元或大於500萬元");
		return;
	}
	var count = eval(document.IP0706000.TXN_AMT.value);
	var balance = eval(document.IP0706000.SEND_AVAL_BALANCE.value);
	if(balance < count){
		alert("轉出帳號可用餘額不足!");
		return;
	}
	else {
		document.IP0706000.SEND_TXN_AMT.value = count;
	}
	var f2=document.IP0706000.SEND_INT_PAY.value;
	var f=document.IP0706000.SEND_INT_CODE.value;
	var m=document.IP0706000.TERM_CODE.value;

	var d="定期存款";
	var z="整存整付";
	var s="存本取息";
	    if(m>0 && m<12){
	    	document.IP0706000.SEND_SC_CODE.value="0";
	    	document.IP0706000.SEND_TITLE.value=d;
	    	document.IP0706000.SEND_SAVING_CODE.value="1";
	    	document.IP0706000.SEND_PERIOD.value=m;
	    	if(f2==1){
	    		if(confirm("轉存期間於11個月內只能選擇每月領息,是否確定繼續執行?")==true){
	    			document.IP0706000.SEND_INT_PAY.value = 2;
	    			document.IP0706000.SEND_PAY_NAME.value="每月領息";
	    		}
	    		else{
	    			return;
	    		}
	    	}
	    }
	    if(m>0 && m>11 && f2==1){
	    	document.IP0706000.SEND_SC_CODE.value="1";
	    	document.IP0706000.SEND_TITLE.value=z;
	    	document.IP0706000.SEND_SAVING_CODE.value="3";
	    	document.IP0706000.SEND_PERIOD.value=m;
	    }
	    if(m>0 && m>11 && f2==2){
	    	document.IP0706000.SEND_SC_CODE.value="0";
	    	document.IP0706000.SEND_TITLE.value=s;
	    	document.IP0706000.SEND_SAVING_CODE.value="2";
	    	document.IP0706000.SEND_PERIOD.value=m;
	    }    
	   //alert(f2 + document.IP0706000.SEND_TITLE.value);
	 
	if (confirm("您所選擇轉存期間為"+m+"個月,定存種類為"+document.IP0706000.SEND_TITLE.value))
		document.IP0706000.SEND_MEMO.value = HalfshapeToFullshape(document.IP0706000.SEND_MEMO.value);
		document.IP0706000.submit();
		
}

