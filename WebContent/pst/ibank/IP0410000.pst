<SCRIPT language="JavaScript">

function chkSubmit(acctStr){

	var acctArray = acctStr.split("@");
   	if ( (!acctArray.length ) || (acctArray.length < 3) || acctArray[2].length < 12 ){
			alert("請選擇帳號！");
			return;      	     
    }
       	
  document.IP0410000.SEND_BR_NAME.value   = acctArray[0];
  document.IP0410000.SEND_ACCT_DESC.value = acctArray[1];
	document.IP0410000.SEND_ACCT_NO.value   = acctArray[2];
	
	document.all.msg.style.visibility="visible";
	document.IP0410000.selAccount.disabled = true;
	document.IP0410000.goBtn.disabled	= true;
	document.IP0410000.submit();
	
	//alert('帳號幣別歸戶中，請稍待');
}
</SCRIPT>
