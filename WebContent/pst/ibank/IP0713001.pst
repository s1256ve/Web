<script language="javascript">

var kbWin = null;
function getKeyBoard()
{
 var Mytype="width=450,height=250,scrollbars=auto,center=yes,border=thin,help=no,status=no";
 var PWD = "window.opener.document.IP0713001.PWD.value";  //show在畫面上的密碼個數
 var KBPWD = "window.opener.document.IP0713001.REVE_PWD.value";  //User真正key的密碼
       kbWin = window.open( "LoadAP?pageid=IP2001000&PWD="+PWD+"&KBPWD="+KBPWD+"&REVE_KB_KEYCODE="+document.IP0713001.REVE_KB_KEYCODE.value+"&REVE_KB_KEYLETTER="+document.IP0713001.REVE_KB_KEYLETTER.value,"newWindow",Mytype);
}


function chkCount(){
	var count = "[[%%SEND-CONTINUE%%]]";
	if (count == 1){
		return "是";
	}
	else {
		return "否";
	}
}

function onTop() {
  	if (kbWin==null) return;
  	if (kbWin != null && (!kbWin.closed)){
  		kbWin.focus();	
  	}
}


function chkSubmit(){
	var acct = "[[%%SEND-ACCT-NO%%]]";
	document.IP0713001.Submit.disabled = true;
	document.IP0713001.Submit2.disabled = true;
	if(acct.substr(0,3)==000 || acct==""){
		alert("您選擇的帳戶非綜存帳戶！");
		chkSubmit2();
		return;
	}
	if(document.IP0713001.REVE_PWD.value==""){
		alert("請輸入網路密碼!");
		document.IP0713001.Submit.disabled = false;
		document.IP0713001.Submit2.disabled = false;
		return;
   	}
   	document.onmousedown=noClick;
	document.IP0713001.submit();
}

function chkSubmit2(){
	document.IP0713001.Submit.disabled = true;
	document.IP0713001.Submit2.disabled = true;
	document.IP0713001.pageid.value = "IP0713000";
   	document.onmousedown=noClick;
	document.IP0713001.submit();
}

function noClick() {
	if ((event.button==1)||(event.button==2)) {
		alert('交易執行中，請稍候 ....');
	}
}
</script>
