    function getTxnType(type){
        if ( type == "" ){
            GType = "全部";    
        }
        if ( type == "SDGAVTRR" ){
            GType = "一般台幣轉帳";    
        }
        if ( type == "SDGAVTRC" ){
            GType = "轉繳本行信用卡";    
        }
        return GType;
    }

    function getSaveDate(sdate){
        var saveDate = "";
        if (sdate=="") return;
        if (sdate.length>8){
            saveDate = getRCDateStr(sdate.substr(0,8),0);
        }
        return saveDate;
    }
    function getChlName(channel){
        if (channel=="") return "其它";
        if (channel=="IBANK"){
            return "網路預約";
        }else{
            return "其它";
        }
        return channel;
    }
    function getDelChannel(delchl){
        if (delchl=="") return "其它";
        if (delchl=="IBANK"){
            return "網路";
        }else{
            return "其它";
        }
        return delchl;
    }
    function BackSubmit(){
        
        document.IP0203201.submit();
    }
</script>
