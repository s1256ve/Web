<script language="JavaScript">

var kbWin = null;


function chkSubmit(){
        document.IP0702401.Submit3.disabled = true;
        document.IP0702401.Submit2.disabled = true;
        if(document.IP0702401.REVE_PWD.value==""){
		alert("請輸入網路密碼");
		document.IP0702401.Submit3.disabled = false;
		return;
        }
        var st = document.IP0702401.REVE_NOTICE_NO.value ;
        var rv = chgcode(st.substring(0,1));
        document.IP0702401.REVE_NOTICE_NO.value = rv + st.substring(1,10);
        document.onmousedown=noClick;     
        document.IP0702401.submit();
}

function getKeyBoard()
{
 var Mytype="width=450,height=250,scrollbars=auto,center=yes,border=thin,help=no,status=no";
 var PWD = "window.opener.document.IP0702401.PWD.value";  //show在畫面上的密碼個數
 var KBPWD = "window.opener.document.IP0702401.REVE_PWD.value";  //User真正key的密碼
       kbWin = window.open( "LoadAP?pageid=IP2001000&PWD="+PWD+"&KBPWD="+KBPWD+"&REVE_KB_KEYCODE="+document.IP0702401.REVE_KB_KEYCODE.value+"&REVE_KB_KEYLETTER="+document.IP0702401.REVE_KB_KEYLETTER.value,"newWindow",Mytype);
}


function onTop() {
  if (kbWin==null) return;
  if (kbWin != null && (!kbWin.closed)){
  	kbWin.focus();	
  }
}

function noClick() {
	if ((event.button==1)||(event.button==2)) {
		alert('交易執行中，請稍候 ....');
	}
}

function chkSubmit2(){
	document.IP0702401.Submit3.disabled = true;
	document.IP0702401.Submit2.disabled = true;
	document.IP0702401.pageid.value = "IP0702410";
	document.onmousedown=noClick;
	document.IP0702401.submit();
}
</script>
