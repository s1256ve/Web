<script>

var kbWin = null;
function getKeyBoard()
{
 var Mytype="width=450,height=250,scrollbars=auto,center=yes,border=thin,help=no,status=no";
 var PWD = "window.opener.document.IP0512001.PWD.value";  //show在畫面上的密碼個數
 var KBPWD = "window.opener.document.IP0512001.REVE_PWD.value";  //User真正key的密碼
       kbWin = window.open( "LoadAP?pageid=IP2001000&PWD="+PWD+"&KBPWD="+KBPWD+"&REVE_KB_KEYCODE="+document.IP0512001.REVE_KB_KEYCODE.value+"&REVE_KB_KEYLETTER="+document.IP0512001.REVE_KB_KEYLETTER.value,"newWindow",Mytype);
}


function onTop() {
  	if (kbWin==null) return;
  	if (kbWin != null && (!kbWin.closed)){
  		kbWin.focus();	
  	}
}


function chkSubmit(){
	document.IP0512001.Submit.disabled = true;
	document.IP0512001.Submit2.disabled = true;
	if(document.IP0512001.REVE_PWD.value==""){
		alert("請輸入網路密碼!");
		document.IP0512001.Submit.disabled = false;
		return;
   	}
   	document.onmousedown=noClick;
	document.IP0512001.submit();
}

function BackSubmit(){
    document.IP0512001.Submit.disabled = true;
    document.IP0512001.Submit2.disabled = true;
    document.IP0512001.pageid.value="IP0512000";
    document.IP0512001.submit();
}

function noClick() {
	if ((event.button==1)||(event.button==2)) {
		alert('交易執行中，請稍候 ....');
	}
}
</script>
