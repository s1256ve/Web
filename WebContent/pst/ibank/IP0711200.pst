<script language="JavaScript">
<!--
var times = 0;//防止重複多次交易
//字串去空白
    String.prototype.Trim=function(){return this.replace(/(^\s*)|(\s*$)/g,"");}
 
var kbWin = null;
function getKeyBoard()
{
 var Mytype="width=230,height=280,scrollbars=auto,center=yes,border=thin,help=no,status=no";
 var PWD = "window.opener.document.IP0711200.PWD.value";  //show在畫面上的密碼個數
 var KBPWD = "window.opener.document.IP0711200.REVE_PWD.value";  //User真正key的密碼
 var DESC = "語音";
 //var DESC = "動態簡訊";
 kbWin = window.open("LoadAP?pageid=IP2001003&PWD="+PWD+"&KBPWD="+KBPWD+"&DESC="+DESC+"&REVE_DESC=&REVE_KB_KEYCODE="+document.IP0711200.REVE_KB_KEYCODE.value+"&REVE_KB_KEYLETTER="+document.IP0711200.REVE_KB_KEYLETTER.value,"newWindow",Mytype);
}


function onTop() {
  	if (kbWin==null) return;
  	if (kbWin != null && (!kbWin.closed)){
  		kbWin.focus();	
  	}
}

function ChkSubmit(){
	document.IP0711200.Submit1.disabled = true;
	document.IP0711200.Submit2.disabled = true;
	var f=document.IP0711200;
	if (f.REVE_PWD.value.Trim() == ""){
		alert("請輸入網路密碼");
		document.IP0711200.Submit1.disabled = false;
		return;
	}
	times++;
	if(times==1){
		document.onmousedown=noClick;
		f.submit();
	}     
}

function noClick() {
	if ((event.button==1)||(event.button==2)) {
		alert('交易執行中，請稍候 ....');
	}
}
    
function ChkSubmit2(){
	document.IP0711200.Submit1.disabled = true;
	document.IP0711200.Submit2.disabled = true;
	document.IP0711200.pageid.value = "IP0711000";
	document.IP0711200.submit();
}

function alertMsg(){
	var m = "[[%%SEND-MPHONE%%]]";
	if (m == ""){
		alert("您未在聯邦銀行存留手機資料，無法使用簡訊做SSL交易。");
		document.IP0711200.Submit1.disabled = true;
		document.IP0711200.Submit2.disabled = true;
		document.IP0711200.pageid.value = "IP0711000";
		document.IP0711200.submit();
	}
	else{
		alert("系統將產生一組動態密碼至您的手機門號" + m + "，若2分鐘內未輸入密碼或收到密碼，本交易將自動取消。");
	}
}
//-->
</script>
