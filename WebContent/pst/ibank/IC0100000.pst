var cn;
var sn = '[[%%SEND-ID-NO%%]]';
function showCName(){
   var cname='[[%%SEND-CNAME%%]]';
   var sex = sn.substring(1,2);
   var title = "";
   if(sex==1){
   		title = "先生";
   }
   else if(sex==2){
   		title = "小姐";
   }
   if(cname.length < 4){
   		if(title!=""){
   			cname = cname.substring(0,1)+title;
   		}
   		else{
   		 cname = cname.substring(0,1)+replaceWord(cname.length-1);
   		}
   }
   else{
   		if(title!=""){
   			cname = cname.substring(0,2)+title;
   		}
   		else{
   		 	cname = cname.substring(0,2)+replaceWord(cname.length-2);
   		}
   }   
   if(cname.length > 8){      
      if(title!=""){
      	cn = cname;
      	return cname;
      }else{
      	cn = cname.substring(0,8)+"(略)";
      	return cname.substring(0,8)+"(略)";
      }
   }else{
      cn = cname;
      return cname;
   }
}

function replaceWord(num){
	var sign = 'O';
	var rtn = "";
	for(i=0;i<num;i++){
		rtn = rtn + sign;
	}
	return rtn;
}

function setYesgogogo() {
//debugger
  //alert("yesgogogo");
  var url = "./pst/yesgogogo/YSGogogoSerGen.jsp"; //產生序號
  var sessid="[[%%SESSIONID%%]]";
  var aptype="IBANK";
  var pars = "sessid="+sessid+"&aptype="+aptype;
  //alert("pars: " + pars);
  new Ajax.Request(url, 
       {   asynchronous:false,
           method: 'post',
           parameters: pars,
           onSuccess: function(transport) {
               ret_setYesgogogo(transport);
           }
       }
   );
}

function ret_setYesgogogo(resp) {
	var msg = resp.responseText;
	//alert(msg);
  var msgArray = msg.split(":");
  //"YESGOGOGO:FAIL:"+msgid+":"+msgdesc+":"+retncode+":"+retndesc
  
    if(resp.responseText.indexOf("YESGOGOGO:OK")>=0)  {  //yesgogogo網站
    	  var ubser=msgArray[2];
    	  //alert(ubser);        
        window.open('https://web.ubot.com.tw/UB/yesgogogo/mybank.aspx?UBBankSer='+ubser,'','menubar=no,scrollbars=yes,resizable=yes');
        //測試環境
        //window.open('http://219.87.58.27/ubot/UB/yesgogogo/mybank.aspx?UBBankSer='+ubser,'','menubar=no,scrollbars=yes,resizable=yes');        
        return;
    }
    if(resp.responseText.indexOf("YESGOGOGO:FAIL")>=0)  {  //逾時或錯誤訊息頁面  
    	//alert("msgid: " + msgArray[2] + " msgdesc: " + msgArray[3] + " retncode: " + msgArray[4] + " retndesc: " + msgArray[5]);
        if(msgArray[2]=="0000"){
        	if(msgArray[4]!="0000"){
        		document.IC0100000.reve_errorcode.value=msgArray[4];
        		document.IC0100000.reve_errormsg.value=msgArray[5];
        	}
        }else{
        		document.IC0100000.reve_errorcode.value=msgArray[2];
        		document.IC0100000.reve_errormsg.valuemsgArray[3];        
        }
        var errorcode=document.IC0100000.reve_errorcode.value;
        var errormsg=document.IC0100000.reve_errormsg.value;
        //alert(errorcode + " " + errormsg);
        window.frames["submain"].location.href="LoadAP?aptype=NOAP&pageid=IE9999006&ERRORCODE="+errorcode+"&ERRORMSG="+errormsg;
        return;        
    }
}

