<script language="javascript">
function AMT(a1,a2){
	var iAMT= parseFloat(a1) + parseFloat(a2);
	return iAMT.toString();
}
function chksubmit1(nextPageid){	
	if (document.IF0201211.REVE_PWD.value != ""){
	var ShortName ="[[%%SEND-CNAME-SHORT%%]]";
			//disableButton2(document.IF0201211.Submit1,document.IF0201211.Submit2);	
			document.IF0201211.pageid.value = nextPageid;
			document.IF0201211.REVE_FEE_RATE.value = [[%%REVE-OFEE-RATE%%]];
			document.IF0201211.REVE_FEE_AMT.value = [[%%REVE-OFEE-AMT%%]];
			document.IF0201211.REVE_CFEE_AMT.value = [[%%REVE-FEE-AMT%%]];
			document.IF0201211.SEND_CNAME_SHORT.value = ShortName.substring(0,4);
			document.IF0201211.REVE_TXN_AMT.value = AMT("[[%%REVE-FUND-AMT%%]]","[[%%REVE-FEE-AMT%%]]");
			document.IF0201211.submit();
	}else{
		alert("請輸入密碼!");
	}		
}

var kbWin = null;
function getKeyBoard()
{
 var Mytype="width=230,height=280,scrollbars=auto,center=yes,border=thin,help=no,status=no";
 var PWD = "window.opener.document.IF0201211.PWD.value";  //show在畫面上的密碼個數
 var KBPWD = "window.opener.document.IF0201211.REVE_PWD.value";  //User真正key的密碼
 var DESC = "語音";
 //var DESC = "動態簡訊";
 kbWin = window.open("LoadAP?pageid=IP2001003&PWD="+PWD+"&KBPWD="+KBPWD+"&DESC="+DESC+"&REVE_DESC=&REVE_KB_KEYCODE="+document.IF0201211.REVE_KB_KEYCODE.value+"&REVE_KB_KEYLETTER="+document.IF0201211.REVE_KB_KEYLETTER.value,"newWindow",Mytype);
}

var email = "[[%%SEA%%]]";
function alertMsg(){    	
	var m = "[[%%REVE-MOBILE%%]]";
	if (m == ""){
		alert("您未在聯邦銀行存留手機資料，無法使用簡訊做授權交易。");
		document.IF0201211.pageid.value = "IP0803001";
		document.IF0201211.submit();
	}
	else if(email==""){
		alert("您未於聯邦銀行留存電子郵件資料，將無法進行網路銀行或樂活APP之基金交易(包含申購、轉換、贖回及變更交易)，僅能執行查詢作業。如有相關疑問，請致電本行24小時客服專線02-25451788或洽本行各營業單位理財顧問。");
		document.IF0201211.pageid.value = "IP0803001";
		document.IF0201211.submit();
	}
	else{
		alert("系統將產生一組簡訊通知交易授權碼至您的手機門號" + m + "，\n若2分鐘內未輸入密碼，本交易將自動取消。");
	}
}

function chksubmit2(nextPageid){
			document.IF0201211.pageid.value = nextPageid;			
			document.IF0201211.submit();
}
</script>
