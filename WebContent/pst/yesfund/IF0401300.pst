<script language="javascript">
function dispExchange(Cury1,Cury2,ExRate){
	if (Cury1 == Cury2){		
		return "1";
	}else{
		return ExRate;	
	}
}
function chkSubmit(){

	if([[%%SEND-CUT-YORN%%]] == 1)
	{
		ans = confirm("是否繼續扣款? 繼續扣款請按「確定」，停止扣款請按「取消」")
		if(ans)
			document.IF0401300.SEND_CUT_YORN.value = "Y";
		else
		{
				ans = confirm("此基金若全部贖回則無法繼續或恢復扣款")
		if(ans)
			document.IF0401300.SEND_CUT_YORN.value = "N";
		else
		{
			disableButton2(document.IF0401300.Submit, document.IF0401300.Submit2);
			document.IF0401300.pageid.value = "IF0401000";
			document.IF0401300.submit();
			return;
		}
		}
	}
	else if([[%%SEND-CUT-YORN%%]] == 2)
	{
		ans = confirm("此基金若全部贖回則無法繼續或恢復扣款")
		if(ans)
			document.IF0401300.SEND_CUT_YORN.value = "N";
		else
		{
			disableButton2(document.IF0401300.Submit, document.IF0401300.Submit2);
			document.IF0401300.pageid.value = "IF0401000";
			document.IF0401300.submit();
			return;
		}
	}
	else
		document.IF0401300.SEND_CUT_YORN.value = "E";
		
	disableButton2(document.IF0401300.Submit, document.IF0401300.Submit2);
	document.IF0401300.pageid.value = "IF0401110";
	document.IF0401300.REVE_CVT_TYPE.value = "1";
	document.IF0401300.submit();
}

function chkSubmit2(){
	disableButton2(document.IF0401300.Submit, document.IF0401300.Submit2);
	document.IF0401300.pageid.value = "IF0401101";
	document.IF0401300.REVE_CVT_TYPE.value = "2";
	document.IF0401300.submit();
}

</script>
