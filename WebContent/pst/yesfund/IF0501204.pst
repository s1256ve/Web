			FAcctAry0[iFAcct] = AcctNo;
			FAcctAry1[iFAcct] = AcctNo + "@" + AcctCury + "@" + AcctBlance;		
			iFAcct++;
			document.IF0501204.AppType[1].disabled = false;
		}
	}else{																     // 台幣帳號
		LAcctAry0[iLAcct] =	AcctNo;
		LAcctAry1[iLAcct] =	AcctNo + "@" + "NTD" + "@" + AcctBlance;
		iLAcct++ ;
	} 
	*/
}

