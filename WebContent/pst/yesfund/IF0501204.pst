 function clearSelect(list){
		var index=list.options.length -1;
		for(; index >= 0 ; index--){
			list.options.remove(index);			
		}		
}

function addOption(list, text, value){
		var index=list.options.length;
		for (i = 0 ; i<text.length ; i++){
			if (text.indexOf('\n')>=0){
				text = text.substr(text.indexOf('\n')+1,text.length); 
			}else{
				break;
			}
		}
		list.options[index]=new Option(text, value);
}	

function chkDuplicateAcct(AcctNo){
	
	for(var j = 0; j < FAcctAry0.length; j++){
		if (FAcctAry0[j] == AcctNo ) return true;	
	}
	return false;	
}

function chkAcctType(AcctNo,AcctCury,AcctBlance){	
	var sCuryCode = AcctNo.substring(3,3+2)
	if (!chkDuplicateAcct(AcctNo)){
		if(sCuryCode != "77")
		{
			if(document.IF0501204.REVE_CERT_NO.value.substring(0,1) != "F" && document.IF0501204.REVE_CERT_NO.value.substring(0,1) != "S" && IF0501204.REVE_CERT_NO.value.substring(0,1) != "P" && document.IF0501204.REVE_CERT_NO.value.substring(0,1) != "W")
			{
			FAcctAry0[iFAcct] = AcctNo;								// 台幣帳號
			FAcctAry1[iFAcct] = AcctNo + "@" + AcctCury + "@" + AcctBlance;		
			iFAcct++;
			}
		}
		else	if(sCuryCode == "77" && ("F" == document.IF0501204.REVE_CERT_NO.value.substring(0,1) || "S" == document.IF0501204.REVE_CERT_NO.value.substring(0,1) || "P" == document.IF0501204.REVE_CERT_NO.value.substring(0,1) || "W" == document.IF0501204.REVE_CERT_NO.value.substring(0,1)))
		{
		FAcctAry0[iFAcct] = AcctNo;								//外幣帳號
		FAcctAry1[iFAcct] = AcctNo + "@" + AcctCury + "@" + AcctBlance;		
		iFAcct++;
		}
		}
