<title>iPASS PAY約定連結聯邦存款帳戶付款</title>
<SCRIPT language="JavaScript">

var reveweb = "[[%%REVE_WEB%%]]";

function chkSubmit(){
	if(document.IP000000A.checkbox1.checked){
		document.IP000000A.submit();
	}
	else{
		alert("您尚未同意約定事項！");
		return;
	}
}
function back_old(){
	if(confirm("是否確定取消帳戶綁定交易，如您確定取消將會回到登入畫面，請您再回到IPASS進行後續作業。")){
		closeWindow();
	}
}

function back(){
	if(reveweb=="TRUE"){
		window.location.href="https://www.ipasspay.com.tw/Views/payment-settings.aspx";
	}else{
		document.IP000000A.action = "IPASS003.jsp";
  		document.IP000000A.submit();
	}
}
