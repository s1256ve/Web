<script language="JavaScript">
function chkSubmit(Pageid){
 	document.IP0801302.pageid.value = Pageid;
 	if(document.IP0801302.REVE_WISH[0].checked){
		document.IP0801302.REVE_WISH_ANS.value = "2";
	}else if(document.IP0801302.REVE_WISH[1].checked){
		document.IP0801302.REVE_WISH_ANS.value = "3";
	}else{
		alert("您尚未選擇是否啟用非約轉！");
		return;
	}
	var PWD1 = document.IP0801302.PWD.value
	var PWD2 = document.IP0801302.PWD2.value
	var sNickName = document.IP0801302.NickName.value
	if (chkPWD(PWD1,PWD2)  && chkNickName(sNickName))  {
	    if (document.IP0801302.email.value.trim() == ""){
    	    var rt = confirm('您尚未填妥Email資料，本行將無法提供您部份\n利用Email通知之網路銀行通知服務！您可按"取\n消"回上頁填寫，或待您有Email帳號後，再至網\n路銀行內登錄！');
    	    if (!rt){
    	        return rt;
    	    }
    	}else{
    	    var rtnEmail = chkEmail(document.IP0801302.email.value.trim())
    		if(!rtnEmail){
    			return rtnEmail;	
    		}
    	}	
		document.IP0801302.submit();		
	}
    
}

function chkNickName(sNickName){

	if (sNickName.trim() == ""){
		alert("請輸入網路銀行使用者代號");
		return false;	
	}else if((sNickName.length < 6) || (sNickName.length > 12)){ 
		alert("網路銀行使用者代號請輸入6-12碼");
		return false;
	}else{
		return true;
	}
}

function chkPWD(sPWD1,sPWD2){
	if(sPWD1 != sPWD2){
		alert("密碼和確認密碼不同\n請重新輸入密碼");
		return false;
	}else if(sPWD1 == "" || sPWD2 == ""){
		alert("請輸入密碼和確認密碼");
		return false;
	}else{
		return true;
	} 
}
function chkSubmit2(Pageid){
	document.IP0801302.pageid.value = Pageid;
	document.IP0801302.submit();
}

function KeyPressChk(){
	var iKeyCode = window.event.keyCode
	if (!(iKeyCode >= 48 && iKeyCode <=57) && !(iKeyCode >= 65 && iKeyCode <=90) && !(iKeyCode >= 97 && iKeyCode <=122)){
		window.event.keyCode = 0;
	}
}

function showRule(){
    window.open("https://web.ubot.com.tw/ub/ebank_5/page04_2.asp");
}

function chkNum2(sid){
	var oldID = sid;
	var newID = "";
	for(i=0;i<oldID.length-1;i++){
		if(i>3&&i<8){
			newID = newID + '*';
		}else{
			newID = newID + oldID.charAt(i);
		}
	}
	return newID;
}

function chkAcct(act){
	var oldact = act;
	var newact = "";
	for(i=0;i<oldact.length;i++){
		if(i>3&&i<9){
			newact = newact + '*';
		}else{
			newact = newact + oldact.charAt(i);
		}
	}
	return newact;
}
</script>
