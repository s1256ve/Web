<script language="javaScript">
	var isFirstLoad = true;

	function checkLoaded() {
		

		if (!document.getElementById){
			clearInterval(Timeout_ID);
			Timeout_ID = null;
			alert("IE failed:document.getElementById");
			return;
		}

		if (document.getElementById("left").document.readyState == "complete" &&
		    document.getElementById("main").document.readyState == "complete" &&
		    document.getElementById("down").document.readyState == "complete") {	
		    
			clearInterval(Timeout_ID);
			Timeout_ID = 0;	

			if ( (document.getElementById) && 
	  		     (document.getElementById("left")) && (document.getElementById("left").style) &&
	  		     (document.getElementById("main")) && (document.getElementById("main").style) &&
	  		     (document.getElementById("down")) && (document.getElementById("down").style) ) {
	  			
				var bodyLeft = window.frames["left"].document.body; 
				var bodyMain = window.frames["main"].document.body;
				var bodyDown = window.frames["down"].document.body;
	
	  			var iLeft = document.getElementById("left").style;
	  			var iMain = document.getElementById("main").style;
	  			var iDown = document.getElementById("down").style;
	  			
						
	  			if (isFirstLoad) {
		  			iMain.height = bodyMain.scrollHeight;
		  			iMain.width  = bodyMain.scrollWidth;

		  			iLeft.height = bodyLeft.scrollHeight;
		  			iLeft.width  = bodyLeft.scrollWidth;
					
		  			iDown.height = bodyDown.scrollHeight;
		  			iDown.width  = bodyDown.scrollWidth;

		  			iMain.left = iLeft.width;
						iDown.top  = parseInt(iMain.top, 10) + parseInt(iMain.height, 10);  
					
						isFirstLoad = false;
					}
			
				
	    }
	     		
			if (parent && parent.document.getElementById &&
				document.getElementById("left").document.readyState == "complete" &&
				document.getElementById("main").document.readyState == "complete" &&
				document.getElementById("down").document.readyState == "complete") {
				
				parent.document.getElementById("IC000000F").style.height = parseInt(parseInt(iDown.height, 10) + parseInt(iDown.top, 10)) + "px";

			}else{alert("FAILED: parent.document.getElementById");}
			
		}	
	}
		Timeout_ID = setInterval("checkLoaded()", 1000);
		
</script>

