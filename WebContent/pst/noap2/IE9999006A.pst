		document.getElementById('showbutton').alt='回主選單';
	}else{
		document.IE9999007.pageid.value = "IC0000104";
		document.IE9999007.aptype.value = "NOAP";
		document.getElementById('showbutton').setAttribute('title', "回登入區");
		document.getElementById('showbutton').src='Accessible/button_17.gif';
		document.getElementById('showbutton').alt='回登入區';
	}
}	
