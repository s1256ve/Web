function pwdcheck(pwd){
	var re = /^[A-Za-z0-9]+$/;
	var len = pwd.length;
	if(len<6||len>12){
		alert("您輸入的密碼長度不符，請重新輸入！");
		return false;
	}
	if (!re.test(pwd)){
		alert("密碼欄位僅能接受英數字！");
		return false;
	}else{
		return true;
	}
}

