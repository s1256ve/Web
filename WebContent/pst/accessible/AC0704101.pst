<script language="Javascript">
    
	function chkSubmit(actinfos){
		
	  var f = document.AC0704101;
	  
		
	  var toact =    actinfos.split('@');
	  var fromact =  f.FROMACTINFO.value.split('@');
	  
	  //alert(toact[1]);
	  //alert(fromact[0]);
	  
	 
	  
	  if(toact[1] == fromact[0]){
    		alert("轉入轉出不可為相同帳號!");
    		return;
    	}
	  
	  f.TO_ACCT_NO.value = toact[1];
	  f.TOACTINFOCHK.value = actinfos;
	  f.pageid.value="AC0704102";
      f.TO_CHANGE.value = "2";
	  f.action = "LoadAP3?pageid=AC0704102";
      f.submit();
		
		//alert('f.TOACTINFOCHK.value='+f.TOACTINFOCHK.value);
	}
	
	
	
	function chkSubmit1(actinfos){
		
